<h3>Your search for : {{ searchTerm }}</h3>
<hr>

{{#if hasContent}}
    {{#each img}}
        <div class="card">
            <img src={{this.url}} alt="" class="card-img-top">
            <div class="card-body">
                <h5 class="card-title">{{ this.title }}</h5>
            </div>
        </div>
        
    {{/each}} 
{{else}}
    <h3> Your search returns no result.</h3>
{{/if}}